{
  "name": "sowberry",
  "homepage": "https://sowberry.prisoltech.app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently -n frontend,backend -c cyan,yellow \"vite\" \"cd server && node --watch server.js\"",
